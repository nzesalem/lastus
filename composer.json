{
  "name": "nzesalem/lastus",
  "description": "A simple package to easily add and manage statuses in your laravel models",
  "keywords": [
    "laravel",
    "lastus",
    "status",
    "models"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Salem Nzeukwu",
      "email": "sjimfender@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "^9.19",
    "doctrine/dbal": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^7.6",
    "mockery/mockery": "^1.4.4"
  },
  "autoload": {
    "psr-4": {
      "Nzesalem\\Lastus\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/Middleware/MiddlewareTest.php"
    ],
    "psr-4": {
      "Nzesalem\\Lastus\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Nzesalem\\Lastus\\ServiceProvider"
      ],
      "aliases": {
        "Lastus": "Nzesalem\\Lastus\\Facade"
      }
    }
  }
}
